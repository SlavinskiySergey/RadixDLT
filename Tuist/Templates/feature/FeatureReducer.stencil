import ComposableArchitecture
import Foundation

@Reducer
public struct {{ name }}Reducer {
  @ObservableState
  public struct State: Equatable {
    
    public init() {}
  }
  
  public enum Action {
    case task
  }
  
  public init() {}
  
  public var body: some ReducerOf<Self> {
    Reduce { state, action in
      switch action {
      case .task:
        return .none
      }
    }
  }
}